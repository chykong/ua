<?xml version="1.0" encoding="UTF-8" ?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
	version="2.0">
	<tlib-version>1.0</tlib-version>
	<short-name>bln</short-name>
	<uri>http://www.balance-soft.com/jsp/jstl/tags</uri>

	<function>
		<description>生成菜单</description>
		<name>createMenu</name>
		<function-class>com.critc.util.web.WebTag</function-class>
		<function-signature>java.lang.String createMenu(javax.servlet.http.HttpServletRequest)</function-signature>
	</function>
	<function>
		<description>校验按钮权限</description>
		<name>isP</name>
		<function-class>com.critc.util.web.WebTag</function-class>
		<function-signature>boolean isPrivilege(java.lang.String)</function-signature>
	</function>
	<function>
		<description>Url编码</description>
		<name>encodeUrl</name>
		<function-class>com.critc.util.string.StringUtil</function-class>
		<function-signature>java.lang.String encodeUrl(java.lang.String)</function-signature>
	</function>

	<function>
		<description>格式化日期</description>
		<name>formatDate</name>
		<function-class>com.critc.util.web.WebTag</function-class>
		<function-signature>java.lang.String formatDate(java.util.Date)</function-signature>
	</function>

	<function>
		<description>格式化日期</description>
		<name>formatDate2</name>
		<function-class>com.critc.util.web.WebTag</function-class>
		<function-signature>java.lang.String formatDate2(java.util.Date,java.lang.String)</function-signature>
	</function>
	<function>
		<description>截取日期</description>
		<name>subString</name>
		<function-class>com.critc.util.web.WebTag</function-class>
		<function-signature>java.lang.String subString(java.lang.String)</function-signature>
	</function>
	<function>
		<description>截取长日期</description>
		<name>subLongDate</name>
		<function-class>com.critc.util.web.WebTag</function-class>
		<function-signature>java.lang.String subLongDate(java.lang.String)</function-signature>
	</function>

	<function>
		<description>系统管理-用户管理获取用户状态</description>
		<name>getUserStatus</name>
		<function-class>com.critc.util.web.WebTag</function-class>
		<function-signature>java.lang.String getUserStatus(java.lang.Integer)</function-signature>
	</function>
</taglib>